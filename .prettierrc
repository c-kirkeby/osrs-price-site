{
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "pluginSearchDirs": [
    "."
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    },
    {
      "files": [
        "tsconfig.json"
      ],
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
